[global]
     workgroup = DEV
     server string = %h server (Samba, Ubuntu)
     netbios name = <%= fqdn %>
     dns proxy = no
     log file = /var/log/samba/log.%m
     max log size = 10000
     syslog = 3
     panic action = /usr/share/samba/panic-action %d
     encrypt passwords = true
     security = user
     username map = /etc/samba/smbusers
     unix extensions = no
     wide links = yes

[apci]
    comment = APCI share
    path = /mnt/apci
    browseable = yes
    writable = yes
    guest ok = no
    create mask = 0755
    directory mask = 0755
    valid users = +admin

[users]
    comment = Home Directories
    browseable = yes
    valid users = %S
    writable = yes